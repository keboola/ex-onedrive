{
  "authorization": {
    "oauth_api": {
      "credentials": {
        "#data": "${OAUTH_DATA}",
        "#appSecret": "${OAUTH_APP_SECRET}",
        "appKey": "${OAUTH_APP_ID}"
      }
    }
  },
  "parameters": {
    "output": {
      "table": "sheet-table"
    },
    "workbook": {
      "search": ".xlsx"
    },
    "worksheet": {
      "position": 0
    }
  }
}
